{
  "name": "azure-devops-dashboard-pdf-extension",
  "version": "1.0.0",
  "description": "Export Azure DevOps dashboards to PDF format using React",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "serve": "webpack serve --mode development",
    "package": "tfx extension create --manifest-globs vss-extension.json",
    "publish": "tfx extension publish --manifest-globs vss-extension.json",
    "clean": "rimraf dist *.vsix"
  },
  "compilerOptions": {
    "isolatedModules": false
  },
  "dependencies": {
    "@fluentui/react": "^8.123.1",
    "azure-devops-extension-api": "^4.234.0",
    "azure-devops-extension-sdk": "^3.1.2",
    "azure-devops-ui": "^2.238.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.3",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.3",
    "tfx-cli": "^0.21.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.8.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "keywords": [
    "azure-devops",
    "extension",
    "dashboard",
    "pdf",
    "export",
    "react"
  ],
  "author": "Alexandre Claus",
  "license": "MIT"
}
