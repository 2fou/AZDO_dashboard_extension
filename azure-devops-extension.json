{
    "manifestVersion": 1,
    "id": "dashboard-pdf-generator",
    "publisher": "AgileCoachingSolutionsDashboardAddon",

    "name": "Print your Azure DevOps Dashboard in PDF",
    "description": "This extension adds a new tab to the work item form that shows the deliverables associated with the work item.",
    "public": false,
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    
    "scopes": [
        "vso.work","vso.work_write"
    ],
    "files": [
        {
            "path": "dist",
            "addressable": true
        }
    ]
    
}